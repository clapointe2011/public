FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      combustionProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

combustionModel eddyDissipation;

eddyDissipationCoeffs
{
    semiImplicit no;
    C_EDC   4.0;
    C_Diff  0.4;
    C_Stiff 1e-10;
}

