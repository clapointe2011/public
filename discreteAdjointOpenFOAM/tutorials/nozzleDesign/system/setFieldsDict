FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        box (0 0.1 0)(0.6 0.2 0.1);

        fieldValues
        (
            volVectorFieldValue U (200 0 0)
        );
    }
);

