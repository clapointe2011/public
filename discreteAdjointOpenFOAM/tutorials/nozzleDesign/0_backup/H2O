FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions              [0 0 0 0 0 0 0];

internalField           uniform 0;

boundaryField
{
    "(inlet|fuelInlet|fuelInlet1)"
    {
        type            fixedValue;
	    value           uniform 0;
    }
    outlet
    {
        type            inletOutlet;
	    value		    uniform 0;
	    inletValue	    uniform 0;
    }
    "(upperWall|upperWall2)"
    {
        type            symmetryPlane;
    }
    "(lowerWall|lowerWall2)"
    {
        type            zeroGradient;
    }
}


