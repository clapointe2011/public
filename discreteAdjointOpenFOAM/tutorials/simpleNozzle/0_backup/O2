FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.23;

boundaryField
{
    "(fuelInlet|fuelInlet1)"
    {
        type            fixedValue;
	value		uniform 0;
    }
    inlet
    {
        type            fixedValue;
	value		uniform 0.23;
    }
    outlet
    {
        type            inletOutlet;
	value		uniform 0.23;
	inletValue	uniform 0.23;
    }
    "(upperWall|lowerWall|upperWall2|lowerWall2)"
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


