FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(inlet|fuelInlet|fuelInlet1)"
    {
        type            calculated;
	value		uniform 0;
    }
    outlet
    {
        type            calculated;
	value		uniform 0;
    }
    "(upperWall|lowerWall|upperWall2|lowerWall2)"
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           uniform 0;

	//type		zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


