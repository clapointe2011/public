FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

functions
{
    FieldAverage
    {
        type                fieldAverage;
        libs                ("libfieldFunctionObjects.so");
        writeControl        outputTime;
        restartOnRestart    false;
        restartOnOutput     false;
        periodicRestart     false;
        restartPeriod       0.002;
        fields
        (
            U
            {
                mean            on;
                prime2Mean      off;
                base            time;
                window          10.0;
                windowName      w1;
            }
        );
    }
}


