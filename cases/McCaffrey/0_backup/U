FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    sides
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    base
    {
        type            noSlip;
    }

    inlet
    {
        type            flowRateInletVelocity;
        massFlowRate    constant 0.00115 //57.5 kW, using 5e4 kJ/kg HOC for methane
        rhoInlet        0.66;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
